{
  "name": "d2-crud-plus-test",
  "version": "1.16.2",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "start": "npm run serve",
    "dev": "npm run serve ",
    "dev:pm": "vue-cli-service serve --open --mode pm",
    "dev:test": "vue-cli-service serve --open --mode test",
    "build": "vue-cli-service build --report",
    "build:local": "vue-cli-service build --mode local",
    "build:analyze": "vue-cli-service build --mode analyze",
    "build:preview": "vue-cli-service build --mode preview",
    "lint": "vue-cli-service lint --fix",
    "test": "vue-cli-service test:e2e  --url http://localhost:8080",
    "test.pm": "vue-cli-service test:e2e --mode pm --url http://localhost:8080",
    "test.all": "rimraf ./tests/e2e/results  && vue-cli-service test:e2e  --headless --url http://localhost:8080 && npm run buildReport",
    "test.all.pm": "rimraf ./tests/e2e/results  && vue-cli-service test:e2e --mode pm  --headless --url http://localhost:8080 && npm run buildReport",
    "test.ci1:e2e": "vue-cli-service test:e2e  --headless --mode test  --url http://localhost:8080 --spec tests/e2e/specs/public/login.spec.js",
    "test.ci:e2e": "vue-cli-service test:e2e  --headless --mode test",
    "test.ci.pm:e2e": "vue-cli-service test:e2e  --headless --mode test_pm  --url http://api.veryreader.com:9080/D2CrudPlusExample",
    "test.ci.pm.chrome:e2e": "vue-cli-service test:e2e  --headless --mode test_pm --browser chrome  --url http://api.veryreader.com:9080/D2CrudPlusExample",
    "test.ci.preview:e2e": "vue-cli-service test:e2e --record --key 85c6440c-b4e5-43f3-9ff9-be205cded925 --headless --mode test_pm --url http://qiniu.veryreader.com/D2CrudPlusExample",
    "buildReport": "node tests/e2e/start/index.js",
    "test0:e2e": "vue-cli-service test:e2e --mode test ",
    "test1:e2e": "vue-cli-service test:e2e --mode test_pm ",
    "test3:e2e": "vue-cli-service test:e2e --headless  --url http://localhost:8080 --spec tests/e2e/specs/public/login.spec.js",
    "test4:e2e": "vue-cli-service test:e2e --headless  --url http://qiniu.veryreader.com/D2CrudPlusExample --spec tests/e2e/specs/form/select.spec.js ",
    "test5:e2e": "vue-cli-service test:e2e --mode test_pm --url http://qiniu.veryreader.com/D2CrudPlusExample  ",
    "test6:e2e": "vue-cli-service test:e2e --mode test_pm --browser chrome --url http://qiniu.veryreader.com/D2CrudPlusExample  ",
    "test7:e2e": "vue-cli-service test:e2e --url http://api.veryreader.com:9080/D2CrudPlusExample"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "dayjs": "^1.8.17",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@d2-plus/vue-cli-plugin-e2e-cypress": "4.5.3-4.9.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "fs-extra": "^9.0.1",
    "lint-staged": "^9.5.0",
    "mocha": "^8.1.1",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.1.0",
    "mochawesome-report-generator": "^5.1.0",
    "vue-template-compiler": "^2.6.11"
  }
}
